<%= simple_form_for [@service, @booking] do |f| %>
<%= f.input :starting_date, as: :date, html5: true %>
<%= f.input :end_date, as: :date, html5: true %>
<%= f.hidden_field :service_id, value: @service.id %>
<%= f.hidden_field :user_id , value: @booking.user_id %>

<br>
   <%= f.submit %>
<% end %>
<br>
