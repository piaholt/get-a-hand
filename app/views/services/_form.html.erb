<%= simple_form_for service do |f| %>
  <%= f.input :title %><br><br>
  <%= f.input :description %><br><br>
  <%= f.input :category, collection: Service::CATEGORY  %><br><br>
  <%= f.input :price %><br><br>
  <%= f.input :photo, as: :file %>
  <%= f.submit "submit", class: "btn btn-secondary" %>
<% end %>
