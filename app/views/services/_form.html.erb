<%= simple_form_for service do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br>
  <%= f.label :description %>
  <%= f.text_field :description %> <br>
  <%= f.label :category %>
  <%= f.text_field :category %> <br>
  <%= f.label :price %>
  <%= f.text_field :price %> <br>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Add new service", class: "btn-btn-primary" %>
<% end %>
